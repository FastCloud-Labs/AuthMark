#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/Dan/Code/authmarked/prep/pitch/AuthMark/node_modules/.pnpm/@slidev+cli@0.49.13_@types+markdown-it@14.1.1_postcss@8.4.39/node_modules/@slidev/cli/bin/node_modules:/Users/Dan/Code/authmarked/prep/pitch/AuthMark/node_modules/.pnpm/@slidev+cli@0.49.13_@types+markdown-it@14.1.1_postcss@8.4.39/node_modules/@slidev/cli/node_modules:/Users/Dan/Code/authmarked/prep/pitch/AuthMark/node_modules/.pnpm/@slidev+cli@0.49.13_@types+markdown-it@14.1.1_postcss@8.4.39/node_modules/@slidev/node_modules:/Users/Dan/Code/authmarked/prep/pitch/AuthMark/node_modules/.pnpm/@slidev+cli@0.49.13_@types+markdown-it@14.1.1_postcss@8.4.39/node_modules:/Users/Dan/Code/authmarked/prep/pitch/AuthMark/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/Dan/Code/authmarked/prep/pitch/AuthMark/node_modules/.pnpm/@slidev+cli@0.49.13_@types+markdown-it@14.1.1_postcss@8.4.39/node_modules/@slidev/cli/bin/node_modules:/Users/Dan/Code/authmarked/prep/pitch/AuthMark/node_modules/.pnpm/@slidev+cli@0.49.13_@types+markdown-it@14.1.1_postcss@8.4.39/node_modules/@slidev/cli/node_modules:/Users/Dan/Code/authmarked/prep/pitch/AuthMark/node_modules/.pnpm/@slidev+cli@0.49.13_@types+markdown-it@14.1.1_postcss@8.4.39/node_modules/@slidev/node_modules:/Users/Dan/Code/authmarked/prep/pitch/AuthMark/node_modules/.pnpm/@slidev+cli@0.49.13_@types+markdown-it@14.1.1_postcss@8.4.39/node_modules:/Users/Dan/Code/authmarked/prep/pitch/AuthMark/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@slidev/cli/bin/slidev.mjs" "$@"
else
  exec node  "$basedir/../@slidev/cli/bin/slidev.mjs" "$@"
fi
